<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginStart="@dimen/item_IOEnglish_cv_main_marginStartEnd"
    android:layout_marginTop="@dimen/item_IOEnglish_cv_main_marginStartEnd"
    android:layout_marginEnd="@dimen/item_IOEnglish_cv_main_marginStartEnd"
    android:layout_marginBottom="@dimen/item_IOEnglish_cv_main_marginStartEnd"
    android:elevation="@dimen/card_view_elevation"
    android:background="@drawable/color_ag_card_rounded"
    app:cardCornerRadius="@dimen/item_tag_cv_corner_radius"
    >

    <TextView
        android:id="@+id/tv_phrase_in_japanese"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/primary_text_color"
        android:textSize="@dimen/item_IOEnglish_phrase_text_Size"
        tools:text="tag"
        android:padding="@dimen/card_view_margin"/>

</LinearLayout>